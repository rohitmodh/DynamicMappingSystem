{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MappingRuleSource": "json",
  "DataFormatSource": "json",
  "ConnectionStrings": {
    "MongoDbConnectionStringR": "mongodb://localhost:27017"
  },
  "MappingRules": {
    "Provider": "Json", // or "Mongo"
    "JsonMappingRuleFilePath": "Resources/MappingRules/mappingrules.json",
    "JsonDataFormatFilePath": "Resources/MappingRules/dataformat.json",
    "Mongo": {
      "MongoDatabase": "MyDb",
      "CollectionName": "MappingRules"
    }
  }
}
